<!DOCTYPE html>
<html>
<head>
    <% for (var chunk in htmlWebpackPlugin.files.css) { %><link rel="preload" href="<%= htmlWebpackPlugin.files.css[chunk] %>" as="style"><% } %>
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %><link rel="preload" href="<%= htmlWebpackPlugin.files.chunks[chunk].entry %>" as="script"><% } %>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="msapplication-tap-highlight" content="no" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1, maximum-scale=1, minimum-scale=1, width=device-width" />

    <title>Simulation Pass Numériques</title>

    <link rel="canonical" href="https://passnum.societenumerique.gouv.fr" />
    <meta name="description" content="Permettre à chacun d’acquérir des compétences numériques de base." />

    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://passnum.societenumerique.gouv.fr" />
    <meta property="og:title" content="Simulation Pass Numériques" />
    <meta property="og:description" content="Permettre à chacun d’acquérir des compétences numériques de base." />
    <meta property="og:image" content="https://passnum.societenumerique.gouv.fr/images/sharer.png" />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="627" />

    <meta property="fb:pages" content="543099962534877" />

    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="Simulation Pass Numériques" />
    <meta name="twitter:description" content="Permettre à chacun d’acquérir des compétences numériques de base." />
    <meta name="twitter:image:src" content="https://passnum.societenumerique.gouv.fr/images/sharer.png" />

    <script type="application/ld+json">
        {
            "@context": "http://schema.org",
            "@type": "WebSite",
            "url": "https://passnum.societenumerique.gouv.fr",
            "name": "Simulation Pass Numériques",
            "image": "https://passnum.societenumerique.gouv.fr/images/sharer.png",
            "description": "Permettre à chacun d’acquérir des compétences numériques de base."
        }
    </script>
</head>
<body data-env="<%= htmlWebpackPlugin.options.environment %>">
    <div id="app"></div>
</body>
</html>
